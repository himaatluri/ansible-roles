global:
  scrape_interval: 15s
  evaluation_interval: 15s

scrape_configs:
  - job_name: 'prometheus'
    static_configs:
      - targets:
          - "{{ ansible_default_ipv4.address }}:9090"

  - job_name: 'node_exporter'
    static_configs:
      - targets:
          - "{{ ansible_default_ipv4.address }}:9100"